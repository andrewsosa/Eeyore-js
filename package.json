{
  "name": "eeyore",
  "description": "Retrieve your (or someone else's) pinned Github repos!",
  "version": "2.0.2",
  "author": "Andrew Sosa <andrew@andrewsosa.com> (http://andrewsosa.com)",
  "bugs": {
    "url": "https://github.com/andrewsosa/eeyore/issues",
    "email": "andrew@andrewsosa.com"
  },
  "contributors": [
    "Andrew Sosa <andrew@andrewsosa.com> (http://andrewsosa.com)"
  ],
  "dependencies": {
    "aimer": "1.3.0"
  },
  "ava": {
    "failFast": true,
    "verbose": true
  },
  "devDependencies": {
    "auto-bind": "^1.1.0",
    "ava": "^0.22.0",
    "codecov": "^3.6.5",
    "cross-env": "^5.0.5",
    "eslint": "^4.5.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.4",
    "nyc": "^11.1.0",
    "prettier": "^1.6.1",
    "remark-cli": "^4.0.0",
    "remark-preset-github": "^0.0.6",
    "standard": "latest"
  },
  "engines": {
    "node": ">=8.3"
  },
  "homepage": "https://github.com/andrewsosa/eeyore",
  "keywords": [
    "github",
    "repos",
    "pinned-repos",
    "pins",
    "nodejs",
    "javascript"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx,mjs,ts,tsx,css,less,scss,json,graphql}": [
      "prettier --write --single-quote --trailing-comma none",
      "git add"
    ],
    "*.md": [
      "remark . -qfo",
      "git add"
    ]
  },
  "main": "index.js",
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100,
    "reporter": [
      "lcov",
      "html",
      "text"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "preset-github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andrewsosa/eeyore"
  },
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "standard",
    "lint-2": "standard && remark . -qf",
    "precommit": "lint-staged && npm test",
    "test": "npm run lint && npm run test-coverage",
    "test-coverage": "cross-env NODE_ENV=test nyc ava",
    "preversion": "npm test",
    "version": "git add -u",
    "postversion-manual": "git push && git push --tags"
  }
}
